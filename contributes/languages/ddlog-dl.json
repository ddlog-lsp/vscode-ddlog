{
  "autoCloseBefore": ") \n\t",
  "autoClosingPairs": [
    { "open": "(;", "close": ";", "notIn": ["string"] },
    { "open": "(", "close": ")", "notIn": ["comment", "string"] },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "brackets": [["(", ")"]],
  "comments": {
    "lineComment": ";;",
    "blockComment": ["(;", ";)"]
  },
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ]
}
